&msm_gpio{
/delete-node/  cdc-reset-line;
 /delete-node/ pmx_sdc3_clk;
/delete-node/  pmx_sdc3_cmd;
/delete-node/ pmx_sdc3_data;
	goodix_reset: goodix_reset {
			goodix_reset_active: active {
				mux {
					pins = "gpio110";
					function = "gpio";
				};
				config {
					pins = "gpio110";
					drive-strength = <2>;
					bias-disable;
					output-high;
				};
			};

			goodix_reset_reset: reset {
				mux {
					pins = "gpio110";
					function = "gpio";
				};
				config {
					pins = "gpio110";
					drive-strength = <2>;
					bias-disable;
					output-low;
				};
			};
	};



	pmx_mdss: pmx_mdss {
			mdss_dsi_active: mdss_dsi_active {
				mux {
					pins = "gpio25";
					function = "gpio";
				};

				config {
					pins = "gpio25";
					drive-strength = <2>; /* 8 mA */
					bias-disable = <0>; /* no pull */
					output-high;
				};
			};

			mdss_dsi_suspend: mdss_dsi_suspend {
				mux {
					pins = "gpio25";
					function = "gpio";
				};

				config {
					pins = "gpio25";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
					output-low;
				};
			};
		};

	goodix_irq: goodix_irq  {
			goodix_irq_active: active {
				mux {
					pins = "gpio106";
					function = "gpio";
				};
				config {
					pins = "gpio106";
					drive-strength = <2>;
					bias-disable;
					input-enable;
				};
			};

	};



tlmm_gpio_key {
			gpio_key_active: gpio_key_active {
				mux {
					pins = "gpio112", "gpio113";
					function = "gpio";
				};

				config {
					pins = "gpio112", "gpio113";
					drive-strength = <2>;
					bias-pull-up;
				};
			};

			gpio_key_suspend: gpio_key_suspend {
				mux {
				pins = "gpio112", "gpio113";
					function = "gpio";
				};

				config {
					pins = "gpio112", "gpio113";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};



	cam_sensor_rear_default: cam_sensor_rear_default {
			/* RESET, STANDBY */
			mux {
				pins = "gpio35", "gpio36", "gpio143", "gpio144";
				function = "gpio";
			};

			config {
				pins = "gpio35", "gpio36", "gpio143", "gpio144";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

	cam_sensor_rear_sleep: cam_sensor_rear_sleep {
			/* RESET, STANDBY */
			mux {
				pins = "gpio35", "gpio36", "gpio143", "gpio144";
				function = "gpio";
			};

			config {
				pins = "gpio35", "gpio36", "gpio143", "gpio144";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		/delete-node/ cam_sensor_rear_vana;

		/delete-node/ cam_sensor_rear_vana_sleep;

		cam_sensor_mclk1_default: cam_sensor_mclk1_default {
			/* MCLK1 */
			mux {
				/* CLK, DATA */
				pins = "gpio28";
				function = "cam_mclk";
			};

			config {
				pins = "gpio28";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		cam_sensor_mclk1_sleep: cam_sensor_mclk1_sleep {
			/* MCLK1 */
			mux {
				/* CLK, DATA */
				pins = "gpio28";
				function = "gpio";
			};

			config {
				pins = "gpio28";
				bias-pull-down; /* PULL DOWN */
				drive-strength = <2>; /* 2 MA */
			};
		};

		cam_sensor_front_default: cam_sensor_front_default {
			/* RESET, STANDBY */
			mux {
				pins = "gpio131", "gpio144", "gpio143";
				function = "gpio";
			};

			config {
				pins = "gpio131", "gpio144", "gpio143";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		cam_sensor_front_sleep: cam_sensor_front_sleep {
			/* RESET, STANDBY */
			mux {
				pins = "gpio131", "gpio144", "gpio143";
				function = "gpio";
			};

			config {
				pins = "gpio131", "gpio144", "gpio143";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};
		/delete-node/  cam_sensor_front_vdig;
		/delete-node/  cam_sensor_front_vdig_sleep;
		 /delete-node/ cam_sensor_mclk2_default;
		 /delete-node/ cam_sensor_mclk2_sleep;
		 /delete-node/ cam_sensor_front1_default;
		/delete-node/ cam_sensor_front1_sleep;

