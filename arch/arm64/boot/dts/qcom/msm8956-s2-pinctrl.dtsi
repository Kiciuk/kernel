			config {
				pins = "gpio111";
				bias-pull-up;
				drive-strength = <6>; /* 2 MA */
			};
		};
	audio_switch_suspend: suspend{
			/* RESET, STANDBY */
			mux {
				pins = "gpio111";
				function = "gpio";
			};

			config {
				pins = "gpio111";
				bias-pull-down;
				drive-strength = <2>; /* 2 MA */
			};
		};

	};
